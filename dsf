[32m+[m[32m//[m
[32m+[m[32mGiveValue.prototype.buttonChange = function() {[m
[32m+[m	[32mvar oSelf = this;[m
[32m+[m	[32moSelf.num = $(oSelf.elemen).find('li:nth-child(5) input').val(); //-----------------------oSelf.num---------------[m
[32m+[m	[32m//	console.log(oSelf.num);[m
[32m+[m	[32m$(oSelf.ele).find('.element .minus').on({[m
[32m+[m		[32m'click': function() {[m
[32m+[m			[32moSelf.num--;[m
[32m+[m			[32mif(oSelf.num < 1) {[m
[32m+[m				[32moSelf.num = 1;[m
[32m+[m			[32m}[m
[32m+[m			[32m$(oSelf.elemen).find('li:nth-child(5) input').val(oSelf.num);[m
[32m+[m			[32m//ËÆ°ÁÆó‰ª∑Ê†º[m
[32m+[m			[32moSelf.priceChange();[m
[32m+[m			[32moSelf.iconfontChange();[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m	[32m$(oSelf.ele).find('.element .plus').on({[m
[32m+[m		[32m'click': function() {[m
[32m+[m			[32moSelf.num++;[m
[32m+[m			[32m$(oSelf.elemen).find('li:nth-child(5) input').val(oSelf.num);[m
[32m+[m			[32m//ËÆ°ÁÆó‰ª∑Ê†º[m
[32m+[m			[32moSelf.priceChange();[m
[32m+[m			[32moSelf.iconfontChange();[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//‰ª∑Ê†ºÂèòÂåñ[m
[32m+[m[32mGiveValue.prototype.priceChange = function() {[m
[32m+[m	[32mvar oSelf = this;[m
[32m+[m	[32m//Â∞èËÆ°ÁöÑÂÜÖÂÆπ[m
[32m+[m	[32mvar price1 = $(oSelf.elemen).find('li:nth-child(4) h4').html().slice(1);[m
[32m+[m	[32m//	var num1=$(oSelf.elemen).find('li:nth-child(5) input').val();[m
[32m+[m
[32m+[m	[32moSelf.price = Number(price1) * oSelf.num; //------------------------------------------oSelf.price---------------------------[m
[32m+[m	[32m//		console.log((oSelf.price));[m
[32m+[m
[32m+[m	[32m$(oSelf.elemen).find('li:nth-child(6)').html('Ôø•' + oSelf.price);[m
[32m+[m	[32m$(oSelf.ele).parent().find('.main_b .main_bm p span').html(oSelf.num);[m
[32m+[m	[32m$(oSelf.ele).parent().find('.main_b .main_bm p b').html('Ôø•' + oSelf.price);[m
[32m+[m	[32m$(oSelf.ele).parent().find('.main_b .main_bm p strong').html('Ôø•' + oSelf.price);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//iconfont[m
[32m+[m[32mGiveValue.prototype.iconfontChange = function() {[m
[32m+[m	[32mvar oSelf = this;[m
[32m+[m	[32mvar price = oSelf.price;[m
[32m+[m	[32mvar num = oSelf.num;[m
[32m+[m	[32m$(oSelf.ele).parents('.main_in').find('.iconfont').toggle([m
[32m+[m		[32mfunction() {[m
[32m+[m			[32m$(this).parents('.main_in').find('.iconfont').html(' ').css('border-color', 'red');[m
[32m+[m			[32moSelf.price = 0;[m
[32m+[m			[32moSelf.num = 0;[m
[32m+[m			[32moSelf.priceChange();[m
[32m+[m		[32m},[m
[32m+[m		[32mfunction() {[m
[32m+[m			[32m$(this).parents('.main_in').find('.iconfont').html('&#xe664;').css('border-color', '#fff');[m
[32m+[m			[32moSelf.price = price;[m
