[32m+[m[32m//ËØªÂèñcookieÂπ∂ËµãÂÄº[m
[32m+[m[32mfunction GiveValue(selector) {[m
[32m+[m	[32mthis.ele = $(selector).get(0);[m
[32m+[m	[32mthis.getData();[m
[32m+[m	[32mthis.iconfontChange();[m
[32m+[m	[32mthis.priceChange();[m
[32m+[m	[32mthis.account();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mGiveValue.prototype.getData = function() {[m
[32m+[m	[32mvar oSelf = this;[m
[32m+[m	[32mvar that = $(oSelf.ele).find('.element');[m
[32m+[m	[32m//ÂÖãÈöÜÊ®°ÊùøÂπ∂Âä†Âà∞main_mÂêé[m
[32m+[m	[32mif($.cookie('cart')) {[m
[32m+[m		[32moSelf.elemen = that.clone(true).appendTo(oSelf.ele).css('display', 'block');[m
[32m+[m
[32m+[m		[32m//	console.log(that.size());[m
[32m+[m
[32m+[m		[32moSelf.arr = JSON.parse($.cookie('cart'));[m
[32m+[m		[32m//ÂõæÁâá[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(2) dt img').attr('src', oSelf.arr[0]);[m
[32m+[m		[32m//Ë°£ÊúçÂêçÂ≠ó[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(2) dd p a').html(oSelf.arr[1]);[m
[32m+[m		[32m//ÁºñÂè∑[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(2) dd i span').html(oSelf.arr[2]);[m
[32m+[m		[32m//È¢úËâ≤[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(3) p span').html(oSelf.arr[3]);[m
[32m+[m		[32m//Â∞∫ÂØ∏[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(3) div span').html(oSelf.arr[4]);[m
[32m+[m		[32m//ÊåÇÁâå‰ª∑[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(4) p').html(oSelf.arr[5]);[m
[32m+[m		[32m//ÂîÆ‰ª∑[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(4) h4').html(oSelf.arr[6]);[m
[32m+[m		[32m//Êï∞Èáè[m
[32m+[m		[32m$(oSelf.elemen).find('li:nth-child(5) input').val(oSelf.arr[7]);[m
[32m+[m
[32m+[m		[32m//Âä†ÂáèÊåâÈíÆÂèòÂåñ[m
[32m+[m		[32moSelf.buttonChange();[m
[32m+[m		[32moSelf.iconfontChange();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
