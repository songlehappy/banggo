[32m+[m		[32mfunction() {[m
[32m+[m			[32m$(this).parents('.main_in').find('.iconfont').html('&#xe664;').css('border-color', '#fff');[m
[32m+[m			[32moSelf.price = price;[m
[32m+[m			[32moSelf.num = num;[m
[32m+[m			[32moSelf.priceChange();[m
[32m+[m		[32m}[m
[32m+[m	[32m)[m
