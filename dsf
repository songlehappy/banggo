[32m+[m			[32moSelf.iconfontChange();[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m	[32m$(oSelf.ele).find('.element .plus').on({[m
[32m+[m		[32m'click': function() {[m
[32m+[m			[32moSelf.num++;[m
[32m+[m			[32m$(oSelf.elemen).find('li:nth-child(5) input').val(oSelf.num);[m
